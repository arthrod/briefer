    "prettier": "^2.8.8",
    "turbo": "^1.9.3"
  },
  "resolutions": {
    "y-prosemirror": "^1.2.11",
    "@tiptap/extension-collaboration": "^2.11.5"
  },
  "trustedDependencies": [
    "es5-ext"
  ]
