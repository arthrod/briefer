{
  "name": "briefer",
  "version": "0.0.105",
  "main": "index.js",
  "repository": "git@github.com:/rthrodbriefer.git",
  "private": true,
  "workspaces": ["apps/*", "packages/*"],
  "packageManager": "bun@1.2.2",
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
